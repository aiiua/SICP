{
    "plugins": [
        "-fontsettings",
        "-sharing",
        "-lunr",
        "-search",
        "-highlight",
        "sunlight-highlighter",
        "livereload"
    ],
    "pluginsConfig": {
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        }
    }
}